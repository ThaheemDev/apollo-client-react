{
  "extends": [ "airbnb", "eslint:recommended", "plugin:react/recommended"],
  "env": {
    "es6": true,
    "node": true,
    "jest": true
},
"plugin": ["react"],
"parser": "babel-eslint",
"rules" : {
  "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
   "indent": ["error", "tab"],
   "no-tabs": 0,
   "react/prop-types": 0,
   "react/jsx-indent": [2, "tab"],
   "react/jsx-indent-props": [2, "tab"],
   "react/jsx-one-expression-per-line":false,
   "semi": 2
 }
